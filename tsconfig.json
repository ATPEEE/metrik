{
  "compilerOptions": {
    "strict": true,
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "jsx": "react",
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["src","test"],
  "exclude": [
    "node_modules"
  ]
}
