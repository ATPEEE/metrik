{
	"name": "SEA-4-Key-Metrics-frontend",
	"version": "0.0.1",
	"description": "ThoughtWorks SEA MU beach project - 4 Key Metrics - Frontend",
	"main": "index.js",
	"scripts": {
		"start": "cross-env NODE_ENV=dev webpack serve --config ./scripts/webpack.dev.js",
		"build:dev": "cross-env NODE_ENV=dev webpack --config ./scripts/webpack.dev.js",
		"build:prod": "cross-env NODE_ENV=prod webpack --config ./scripts/webpack.prod.js",
		"build:prod:analyze": "cross-env NODE_ENV=prod ENABLE_ANALYZER=true webpack --config ./scripts/webpack.prod.js",
		"test": "cross-env NODE_ENV=test jest",
		"lint": "eslint --fix --ext .ts,.tsx src",
		"lint:type": "tsc"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "npm test"
		}
	},
	"lint-staged": {
		"*.{js,jex,ts,tsx}": [
			"bash -c tsc",
			"eslint --fix",
			"git add"
		],
		"*.{less,css}": [
			"prettier --write",
			"git add"
		],
		"*.{json}": [
			"prettier --write",
			"git add"
		]
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Hackl0us/SEA-4-Key-Metrics-frontend.git"
	},
	"keywords": [],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/Hackl0us/SEA-4-Key-Metrics-frontend/issues"
	},
	"homepage": "https://github.com/Hackl0us/SEA-4-Key-Metrics-frontend#readme",
	"browserslist": [
		"last 2 versions"
	],
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/plugin-transform-runtime": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@babel/preset-react": "^7.12.10",
		"@babel/preset-typescript": "^7.12.7",
		"@testing-library/jest-dom": "^5.11.8",
		"@testing-library/react": "^11.2.3",
		"@types/jest": "^26.0.20",
		"@types/node": "^14.14.20",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@typescript-eslint/eslint-plugin": "^4.13.0",
		"@typescript-eslint/parser": "^4.13.0",
		"autoprefixer": "^10.2.1",
		"babel-loader": "^8.2.2",
		"babel-plugin-import": "^1.13.3",
		"clean-webpack-plugin": "^3.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^5.0.1",
		"css-minimizer-webpack-plugin": "^1.2.0",
		"eslint": "^7.17.0",
		"eslint-config-prettier": "^7.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-react": "^7.22.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"file-loader": "^6.2.0",
		"glob": "^7.1.6",
		"html-webpack-plugin": "^4.5.1",
		"husky": "^4.3.7",
		"jest": "^26.6.3",
		"less": "^4.1.0",
		"less-loader": "^7.2.1",
		"lint-staged": "^10.5.3",
		"mini-css-extract-plugin": "^1.3.3",
		"postcss-loader": "^4.1.0",
		"prettier": "^2.2.1",
		"purgecss-webpack-plugin": "^3.1.3",
		"style-loader": "^2.0.0",
		"terser-webpack-plugin": "^5.1.1",
		"typescript": "^4.1.3",
		"url-loader": "^4.1.1",
		"webpack": "^5.12.3",
		"webpack-bundle-analyzer": "^4.3.0",
		"webpack-cli": "^4.3.1",
		"webpack-dev-server": "^3.11.1",
		"webpack-merge": "^5.7.3",
		"webpackbar": "^5.0.0-3"
	},
	"dependencies": {
		"@babel/runtime-corejs3": "^7.12.5",
		"@types/webpack": "^4.41.25",
		"antd": "^4.10.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.1"
	}
}
